{
  "name": "codedependency-visualizer",
  "displayName": "Code Dependency Visualizer - Gemini Cursor",
  "description": "Visualizes Python code dependencies using a graph.",
  "version": "0.0.1",
  "publisher": "artinmajdi",
  "repository": "https://github.com/artinmajdi/Project-Template-Manager.git",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codedependency-visualizer.visualizeDependencies"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codedependency-visualizer.visualizeDependencies",
        "title": "Visualize Code Dependencies"
      }
    ],
    "menus": {
        "editor/context": [
            {
                "when": "resourceLangId == python",
                "command": "codedependency-visualizer.visualizeDependencies",
                "group": "navigation"
            }
        ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "18.x",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0"
  },
  "dependencies": {
    "vis-network": "^9.1.9"
  }
}
